    "lint": "eslint .",
    "lint:fix": "yarn lint -- --fix",
    "prebuild": "yarn build:clean",
    "build:clean": "rimraf ./dist/*",
    "build": "cross-env NODE_ENV=production UPLOAD_TO_OSS=true webpack --config webpack/webpack.config.prod.js --color -p",
